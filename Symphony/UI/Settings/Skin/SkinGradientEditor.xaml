<UserControl x:Class="Symphony.UI.Settings.SkinGradientEditor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Symphony.UI.Settings"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <StackPanel>
        <Grid x:Name="gridEditor" Margin="0,10,0,0"/>
        <local:SkinColorEditor x:Name="colorEditor" Margin="-24,5"/>
        <local:SkinGradientStopEditor x:Name="gradientStop"/>
    </StackPanel>
</UserControl>
